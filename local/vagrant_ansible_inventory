# Static Inventory File for Vagrant

meteor ansible_ssh_host=10.100.0.10 ansible_ssh_port=22 ansible_ssh_user='vagrant' ansible_ssh_private_key_file="~/.vagrant.d/insecure_private_key"
dhost ansible_ssh_host=10.100.1.11 ansible_ssh_port=22 ansible_ssh_user='core' ansible_ssh_private_key_file="~/.vagrant.d/insecure_private_key"
dswarm ansible_ssh_host=10.100.1.10 ansible_ssh_port=22 ansible_ssh_user='core' ansible_ssh_private_key_file="~/.vagrant.d/insecure_private_key"

[tuxlab-swarm-manager]
dswarm

[tuxlab-swarm-host]
dhost

[tuxlab-meteor]
meteor
